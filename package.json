{
  "name": "yup-type-support",
  "version": "1.0.3",
  "description": "A typescript support for yup. Able to infer the yup schema from a typescript interface.",
  "main": "./build/index.js",
  "typings": "./dist/index.d.ts",
  "files": [
    "build/**",
    "!build/tests/**"
  ],
  "scripts": {
    "prepublishOnly": "npm run all",
    "all": "npm run build",
    "build": "ttsc",
    "clean": "rm -r build",
    "test": "npm run build; jest --verbose ./build/tests"
  },
  "homepage": "https://github.com/znAaron/yup-type-support",
  "author": "Zinan Guo",
  "license": "MIT",
  "dependencies": {
    "@types/aws-lambda": "^8.10.101",
    "@znaaron/simple-ts-transform": "^1.1.2",
    "yup": "^0.32.11"
  },
  "devDependencies": {
    "@types/jest": "^28.1.6",
    "jest": "^28.1.3",
    "ts-jest": "^28.0.7",
    "ttypescript": "^1.5.13",
    "typescript": "^4.7.4"
  }
}
